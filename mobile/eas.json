    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://api.unspiral.app/api/v1"
      },
      "ios": {
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
